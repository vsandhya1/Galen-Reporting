	partition 'Operations - HR LY' = m
		mode: import
		source =
				let
				    Source = Sql.Database("gln-galendatawarehouse-db1-prod.database.windows.net", "gln-datawarehouse", [Query="DECLARE @LastYearEnd DATE = DATEFROMPARTS(YEAR(GETDATE()) - 1, 12, 31);#(lf)SELECT#(lf)ED.PersonID as person_id,#(lf)ED.EmployeeFullName as employee_full_name,#(lf)ED.EmailAddress as email_address,#(lf)ED.AuxillaryStatus as auxillary_status,#(lf)JD.JobCd as job_cd,#(lf)CASE WHEN JD.JobCd IN (10080, 10081, 10082, 10083, 10085)#(lf)THEN 'Faculty'#(lf)ELSE 'Staff'#(lf)END AS Employee_Type,#(lf)COALESCE(CD.CampusID, 'MAIN') AS CAMPUS,#(lf)COALESCE(CD.CampusLocationDesc, 'MAIN CAMPUS') AS CampusLocationDesc,#(lf)DD.DeptDescHomeCurr as dept_desc_home_curr,#(lf)#(lf)CASE WHEN year(EF.HiredDate) = year(getdate())#(lf)THEN 1#(lf)ELSE 0#(lf)END AS new_hire_indicator,#(lf)EF.HiredDate as date_hired,#(lf)ED.RecordStartDate,#(lf)ED.RecordEndDate#(lf)#(lf)from [dbo].[EmployeeHireFact] EF#(lf)inner join EmployeeDim ED on EF.EmployeeWHKey = ED.EmployeeWHKey --and ED.CurrentInd = 1#(lf) AND  @LastYearEnd BETWEEN ED.RecordStartDate AND ED.RecordEndDate#(lf)left join JobDim JD on EF.JobWHKey = JD.JobWHKey --and JD.CurrentInd = 1#(lf)AND  @LastYearEnd BETWEEN JD.RecordStartDate AND JD.RecordEndDate#(lf)left join DepartmentDim DD on EF.DeptWHKey = DD.DeptWHKey --and DD.CurrentInd = 1#(lf)AND  @LastYearEnd BETWEEN DD.RecordStartDate AND DD.RecordEndDate#(lf)left join SupervisorDim SD on ED.SupervisorWHKey = SD.SupervisorWHKey --and SD.CurrentInd = 1#(lf)AND  @LastYearEnd BETWEEN SD.RecordStartDate AND SD.RecordEndDate#(lf)left JOIN CampusDim CD ON DD.CampusWHKey = CD.CampusWHKey --and CD.CurrentInd = 1#(lf)AND  @LastYearEnd BETWEEN CD.RecordStartDate AND CD.RecordEndDate#(lf)#(lf)where ED.Active = 1 --and EF.CurrentInd = 1#(lf)--and PersonID = '1049491'#(lf)AND  @LastYearEnd BETWEEN EF.RecordStartDate AND EF.RecordEndDate#(lf)"])
				in
				    Source

