role Restrictions
	modelPermission: read

	tablePermission 'Official list for IT' = 'Official list for IT'[Email ID] == USERPRINCIPALNAME()

	tablePermission 'Campus Region' =
			SWITCH(TRUE(),
			'Campus Region'[LOCATIONS_ID] in CALCULATETABLE(VALUES('Official list for IT'[Campus Abbrev]),'Official list for IT'[Email ID]=USERPRINCIPALNAME()
			),True
			)

	tablePermission 'Operations - HR' =
			SWITCH(TRUE(),
			'Operations - HR'[Campus_New] in CALCULATETABLE(VALUES('Official list for IT'[Campus Abbrev])),True
			)

	annotation PBI_Id = 7701ade0ae1d4e46843bcd2d11c7e82f

