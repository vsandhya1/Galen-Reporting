	partition 'Operations - HR DW' = m
		mode: import
		source =
				let
				    Source = Sql.Database("gln-galendatawarehouse-db1-prod.database.windows.net", "gln-datawarehouse", [Query="SELECT#(lf)    ED.PersonID AS person_id,#(lf)    ED.EmployeeFullName AS employee_full_name,#(lf)    ED.EmailAddress AS email_address,#(lf)    ED.AuxillaryStatus AS auxillary_status,#(lf)    JD.JobCd AS job_cd,#(lf)    JD.JobDesc AS job_desc,#(lf)      COALESCE(CD.CampusLocationDesc, 'MAIN CAMPUS') AS CampusLocationDesc,#(lf)      COALESCE(CD.CampusID, 'MAIN') AS Campus,#(lf)    CASE#(lf)        WHEN JD.JobCd IN (10080, 10081, 10082, 10083, 10085) THEN 'Faculty'#(lf)        ELSE 'Staff'#(lf)    END AS Employee_Type,#(lf)#(lf)    DD.DeptDescHomeCurr AS dept_desc_home_curr,#(lf)    CASE#(lf)        WHEN YEAR(EF.HiredDate) = YEAR(GETDATE()) THEN 1#(lf)        ELSE 0#(lf)    END AS new_hire_indicator,#(lf)    EF.HiredDate AS date_hired,#(lf)    ED.RecordStartDate,#(lf)    ED.RecordEndDate#(lf)#(lf)FROM [dbo].[EmployeeHireFact] EF#(lf)INNER JOIN EmployeeDim ED ON EF.EmployeeWHKey = ED.EmployeeWHKey AND ED.CurrentInd = 1#(lf)LEFT JOIN JobDim JD ON EF.JobWHKey = JD.JobWHKey AND JD.CurrentInd = 1#(lf)LEFT JOIN DepartmentDim DD ON EF.DeptWHKey = DD.DeptWHKey AND DD.CurrentInd = 1#(lf)LEFT JOIN SupervisorDim SD ON ED.SupervisorWHKey = SD.SupervisorWHKey AND SD.CurrentInd = 1#(lf)LEFT JOIN CampusDim CD ON DD.CampusWHKey = CD.CampusWHKey AND CD.CurrentInd = 1#(lf)WHERE ED.Active = 1#(lf)AND EF.CurrentInd = 1#(lf)#(lf)"])
				in
				    Source

